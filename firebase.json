{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5000,
      "headers": [{
        "Cache-Control": "public, max-age=31536000, immutable"
      }]
    },
    "ui": {
      "enabled": true
    }
  }
}
